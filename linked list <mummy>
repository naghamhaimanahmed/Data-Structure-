#include<iostream>
using namespace std;
class Node {
public:
    int data;
	Node* next;
};
int main() {
	Node* head = new Node();
    Node* second = new Node();
    Node* third = new Node();
    Node* fourth = new Node();
	Node* last=new Node();
	head->data = 11;
	head->next=second;
	second->data=22;
    second->next=third;
    third->data=33;
    third->next=fourth;
	fourth->data=44;
    fourth->next=last;
    last->data=55;
    last->next=NULL;
   cout << head->data<<second->data<<third->data<<fourth->data<<fourth->next->data<<endl ;
    Node* one = new Node();
    Node* two = new Node();
    Node* three = new Node();
    Node* four = new Node();
    one->data = 111;
	one->next=two;
	two->data=222;
    two->next=three;
    three->data=333;
    three->next=four;
	four->data=444;
    four->next=NULL;
    cout << one->data<<two->data<<three->data<<three->next->data<<endl ;
    Node* five = new Node();
    Node* six = new Node();
    Node* seven = new Node();
    Node* eight = new Node();
	Node* nine =new Node();
	five->data = 555;
	five->next=six;
	six->data=666;
    six->next=seven;
    seven->data=777;
    seven->next=eight;
	eight->data=888;
    eight->next=nine;
    nine->data=999;
    nine->next=NULL;
   cout << five->data<<six->data<<seven->data<<eight->data<<eight->next->data<<endl ;
    Node* n = new Node();
    Node* m = new Node();
    Node* z = new Node();
    Node* q = new Node();
	Node* l=new Node();
	n->data = 1;
	n->next=m;
	m->data=2;
    m->next=z;
    z->data=3;
    z->next=q;
	q->data=4;
    q->next=l;
    l->data=5;
    l->next=NULL;
    cout << n->data<<m->data<<z->data<<q->data<<q->next->data<<endl ;
    Node* ptr1 = new Node();
    Node* ptr2 = new Node();
    Node* ptr3 = new Node();
    Node* ptr4 = new Node();
    ptr1->data = 101;
	ptr1->next=ptr2;
	ptr2->data=202;
    ptr2->next=ptr3;
    ptr3->data=303;
    ptr3->next=ptr4;
	ptr4->data=404;
    ptr4->next=NULL;
    cout << ptr1->data<<ptr2->data<<ptr3->data<<ptr3->next->data<<endl ;
	return 0;
}
